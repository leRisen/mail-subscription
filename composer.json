{
    "name": "le-risen/mail-subscription",
    "type": "lib",
    "version": "1.0.2",
    "license": "MIT",
    "homepage": "https://github.com/leRisen/mail-subscription",
    "description": "A laravel package for subscribe to the newsletter",
    "keywords": [
        "laravel",
        "mail subscription"
    ],
    "authors": [
        {
            "name": "leRisen",
            "email": "lemmas.online@gmail.com",
            "homepage": "https://github.com/leRisen",
            "role": "Developer"
        }
    ],
    "extra": {
        "laravel": {
            "providers": [
                "leRisen\\MailSubscription\\Providers\\MailSubscriptionServiceProvider"
            ]
        }
    },
    "require": {
        "php": ">=7.0",
        "laravel/framework": "~5.5"
    },
    "require-dev": {
        "phpunit/phpunit": "^6.0 || ^7.0",
        "mockery/mockery": "0.9.*",
        "orchestra/database": "^3.5 || ^3.6",
        "orchestra/testbench": "^3.5 || ^3.6"
    },
    "autoload": {
        "psr-4": {
            "leRisen\\MailSubscription\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "leRisen\\MailSubscription\\Tests\\": "tests/"
        }
    },
    "config": {
      "preferred-install": "dist",
      "sort-packages": true,
      "optimize-autoloader": true
    }
}
