{
    "name": "le-risen/mail-subscription",
    "type": "lib",
    "version": "1.0",
    "license": "MIT",
    "homepage": "https://github.com/leRisen/mail-subscription",
    "description": "A laravel package for subscribe to the newsletter",
    "keywords": [
      "laravel",
      "mail subscription"
    ],
    "authors": [
      {
        "name": "leRisen",
        "email": "lemmas.online@gmail.com",
        "homepage": "https://github.com/leRisen",
        "role": "Developer"
      }
    ],
    "extra": {
        "laravel": {
            "providers": [
                "leRisen\\MailSubscription\\MailSubscriptionServiceProvider"
            ]
        }
    },
    "require": {
      "php": ">=5.6.0"
    },
    "autoload": {
      "psr-4": {
        "leRisen\\MailSubscription\\": "src"
      }
    },
    "config": {
      "preferred-install": "dist",
      "sort-packages": true,
      "optimize-autoloader": true
    }
